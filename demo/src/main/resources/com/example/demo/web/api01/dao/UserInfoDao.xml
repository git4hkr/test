<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.web.api01.dao.UserInfoDao">

	<insert id="insert" parameterType="com.example.demo.web.api01.model.UserInfoDto"
		statementType="PREPARED" timeout="180000">
		<![CDATA[
			INSERT INTO T_USERS (id,firstname,lastname,updatetime)
			values
			(#{id},#{firstname},#{lastname},#{updatetime})
		]]>
	</insert>

	<select id="select" resultType="map">
		select * from T_Users
	</select>

</mapper>